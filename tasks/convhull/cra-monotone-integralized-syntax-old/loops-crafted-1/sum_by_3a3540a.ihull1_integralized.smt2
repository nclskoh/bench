(declare-const |j'| Int)
(declare-const j Int)
(declare-const n Int)
(declare-const term_to_project_onto_integralized Int)
(assert (and (<= (- j) 0) (<= (- n) 0) (<= (+ (- n) j 1) 0)
               (= (+ |j'| (- j) -1) 0)
               (= (+ term_to_project_onto_integralized (- (+ |j'| (- j)))) 0)))
(check-sat)