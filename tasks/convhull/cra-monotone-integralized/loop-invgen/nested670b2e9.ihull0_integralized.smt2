(declare-const n Int)
(declare-const i Int)
(declare-const k Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const |k'| Int)
(assert (and (<= (- i) 0) (<= (- k) 0) (<= (+ (- n) 1) 0)
               (<= (+ (- n) k 1) 0) (<= (+ (- k) (* 2 i)) 0)
               (= (+ |k'| (- k) -1) 0)
               (= (+ term_to_project_onto_integralized (- (+ |k'| (- k)))) 0)))
(check-sat)