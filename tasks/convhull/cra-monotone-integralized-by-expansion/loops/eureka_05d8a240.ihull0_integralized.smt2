(declare-const n Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const rh Int)
(declare-const i Int)
(declare-const tr Int)
(declare-const tr2 Int)
(declare-const |rh'| Int)
(declare-const tr1 Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto0_integralized Int)
(declare-const phi_rh Int)
(declare-const tr0 Int)
(assert (and (<= (- rh) 0) (<= (+ (- i) 1) 0) (<= (+ (- n) 1) 0)
               (<= (+ (- n) i 1) 0)
               (or (and (<= (+ (- tr0) tr) 0) (= (+ (- phi_rh) rh) 0))
                     (and (<= (+ (- tr2) tr1 1) 0) (= (+ (- phi_rh) i) 0)))
               (= (+ |i'| (- i) -1) 0) (= (+ |rh'| (- phi_rh)) 0)
               (= (+ term_to_project_onto0_integralized (- (+ |rh'| (- rh)))) 0)
               (= (+ term_to_project_onto_integralized (- (+ |i'| (- i)))) 0)))
(check-sat)