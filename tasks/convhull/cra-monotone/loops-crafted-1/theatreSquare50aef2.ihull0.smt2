(declare-const i Int)
(declare-const term_to_project_onto0 Real)
(declare-const x Int)
(declare-const j Int)
(declare-const |x'| Int)
(declare-const |y'| Int)
(declare-const y Int)
(declare-const term_to_project_onto Real)
(assert (and (<= (- y) 0) (<= (- x) 0) (<= (- j) 0) (<= (- i) 0)
               (<= (+ (- i) x 1) 0) (= (+ |y'| (- j) (- y)) 0)
               (= (+ |x'| (- x) -1) 0)
               (= term_to_project_onto0 (+ |x'| (- x)))
               (= term_to_project_onto (+ |y'| (- y)))))
(check-sat)
