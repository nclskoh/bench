(declare-const x Int)
(declare-const term_to_project_onto Real)
(declare-const |x'| Int)
(declare-const phi_x Int)
(declare-const rem Int)
(declare-const y Int)
(assert (and (<= (- y) 0) (<= (- x) 0) (<= (+ x -98) 0)
               (or (and (or (<= y 0)
                              (and (<= (- rem) 0)
                                     (or (<= (+ rem 1) 0)
                                           (<= (+ (- rem) 1) 0))
                                     (or (<= (+ rem -1) 0) (<= (+ rem 1) 0))
                                     (is_int (/ (+ y (- rem)) 2))))
                          (= (+ (- phi_x) x 1) 0))
                     (and (is_int (/ y 2)) (<= (- y) 0)
                            (= (+ (- phi_x) x 2) 0)))
               (= (+ |x'| (- phi_x)) 0)
               (= term_to_project_onto (+ |x'| (- x)))))
(check-sat)
