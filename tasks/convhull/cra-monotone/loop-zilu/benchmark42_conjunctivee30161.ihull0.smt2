(declare-const |z'| Int)
(declare-const z Int)
(declare-const |y'| Int)
(declare-const term_to_project_onto1 Real)
(declare-const term_to_project_onto0 Real)
(declare-const y Int)
(declare-const x Int)
(declare-const term_to_project_onto Real)
(declare-const |x'| Int)
(assert (and (<= (- x) 0) (<= (- y) 0) (<= z 0) (= (+ z (* 2 y)) 0)
               (= (+ y (- x)) 0) (<= (+ (- x) 1) 0) (= (+ |z'| (- z) -2) 0)
               (= (+ |y'| (- y) 1) 0) (= (+ |x'| (- x) 1) 0)
               (= term_to_project_onto1 (+ |x'| (- x)))
               (= term_to_project_onto0 (+ |y'| (- y)))
               (= term_to_project_onto (+ |z'| (- z)))))
(check-sat)
