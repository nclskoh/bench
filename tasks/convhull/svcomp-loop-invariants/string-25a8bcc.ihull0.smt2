(declare-const j Int)
(declare-const i Int)
(declare-const nc_B Int)
(declare-const nc_A Int)
(assert (exists
         ((|j'?1| Int) (|i'?2| Int) (tr?3 Int) (tr?4 Int) (phi_j?5 Int)
            (phi_i?6 Int) (tr?7 Int) (tr?8 Int))
         (and (<= (- j) 0) (<= (- nc_B) 0) (<= (- nc_A) 0)
                (<= (+ i (- nc_A) 1) 0) (<= (+ (- nc_B) j 1) 0)
                (or (and (or (<= (+ (- tr?7) tr?8 1) 0)
                               (<= (+ tr?7 (- tr?8) 1) 0))
                           (= (+ (- phi_i?6) i (- j) 1) 0) (= (- phi_j?5) 0))
                      (and (= (+ (- tr?3) tr?4) 0) (= (+ (- phi_i?6) i 1) 0)
                             (= (+ (- phi_j?5) j 1) 0)))
                (= (+ |j'?1| (- phi_j?5)) 0) (= (+ |i'?2| (- phi_i?6)) 0))))
(check-sat)
