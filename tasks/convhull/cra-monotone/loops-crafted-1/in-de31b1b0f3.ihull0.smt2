(declare-const y Int)
(declare-const term_to_project_onto Real)
(declare-const |z'| Int)
(declare-const term_to_project_onto0 Real)
(declare-const |y'| Int)
(declare-const z Int)
(assert (and (<= (- z) 0) (<= (- y) 0) (<= (+ (- y) 1) 0)
               (= (+ |z'| (- z) -1) 0) (= (+ |y'| (- y) 1) 0)
               (= term_to_project_onto0 (+ |y'| (- y)))
               (= term_to_project_onto (+ |z'| (- z)))))
(check-sat)
