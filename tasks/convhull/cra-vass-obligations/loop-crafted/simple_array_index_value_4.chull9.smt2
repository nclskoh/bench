(declare-const |loop_entered'| Int)
(declare-const |index1'| Int)
(declare-const |index2'| Int)
(declare-const |tmp'| Int)
(declare-const |param0'| Int)
(assert (exists ((index2?1 Int) (index1?2 Int))
         (or (and (= |loop_entered'| 1) (= |tmp'| 1) (= |param0'| 1)
                    (= |index1'| (+ index1?2 1))
                    (= |index2'| (+ index2?1 -1)))
               (and (= |loop_entered'| 1) (= |tmp'| 1) (= |param0'| 0)
                      (= |index1'| (+ index1?2 1))
                      (= |index2'| (+ index2?1 -1))))))
(check-sat)
