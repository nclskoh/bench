(declare-const |len'| Int)
(declare-const c Int)
(declare-const K Int)
(assert (exists ((len?1 Int))
         (and (= (+ -4 len?1) 0) (<= 0 (+ -1 c))
                (<= (- K) (- (+ |len'| (- len?1))))
                (<= (* -3 K) (+ |len'| (- len?1)))
                (or (and (= K 0) (= len?1 |len'|)) (<= 1 K)) (<= 0 K))))
(check-sat)
