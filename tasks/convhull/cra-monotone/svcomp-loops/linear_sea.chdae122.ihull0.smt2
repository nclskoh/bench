(declare-const tr0 Int)
(declare-const q Int)
(declare-const tr Int)
(declare-const n Int)
(declare-const term_to_project_onto Real)
(declare-const j Int)
(declare-const |j'| Int)
(assert (and (<= (- j) 0) (<= (+ (- n) j 1) 0)
               (or (<= (+ (- q) tr 1) 0) (<= (+ (- tr0) q 1) 0))
               (= (+ |j'| (- j) -1) 0)
               (= term_to_project_onto (+ |j'| (- j)))))
(check-sat)
