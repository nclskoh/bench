(declare-const x Int)
(declare-const term_to_project_onto0 Real)
(declare-const y Int)
(declare-const term_to_project_onto Real)
(declare-const |y'| Int)
(declare-const phi_y Int)
(declare-const |x'| Int)
(assert (and (<= (+ (- y) 1) 0) (<= (- x) 0) (<= (+ x -999999) 0)
               (or (and (<= (+ (- x) 500000) 0) (= (+ (- phi_y) y 1) 0))
                     (and (<= (+ x -499999) 0) (= (+ (- phi_y) y) 0)))
               (= (+ |y'| (- phi_y)) 0) (= (+ |x'| (- x) -1) 0)
               (= term_to_project_onto0 (+ |x'| (- x)))
               (= term_to_project_onto (+ |y'| (- y)))))
(check-sat)
