(declare-const i Int)
(declare-const term_to_project_onto Real)
(declare-const |i'| Int)
(declare-const term_to_project_onto0 Real)
(declare-const sum Int)
(declare-const n Int)
(declare-const |sum'| Int)
(assert (and (<= (- n) 0) (<= (- sum) 0) (<= (- i) 0) (<= (+ i (- n) 1) 0)
               (= (+ |i'| (- i) -1) 0) (= (+ |sum'| (- i) (- sum)) 0)
               (= term_to_project_onto0 (+ |sum'| (- sum)))
               (= term_to_project_onto (+ |i'| (- i)))))
(check-sat)
