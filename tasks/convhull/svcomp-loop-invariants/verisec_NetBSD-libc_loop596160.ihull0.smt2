(declare-const p Int)
(declare-const tmp Int)
(declare-const pathlim Int)
(declare-const p@width Int)
(declare-const p@pos Int)
(assert (exists ((|p@width'?1| Int) (|p@pos'?2| Int) (|p'?3| Int))
         (and (<= (+ (- pathlim) p) 0) (<= (+ (- tmp) p) 0)
                (= (+ (- p@width) |p@width'?1|) 0)
                (= (+ (- p@pos) |p@pos'?2| -4) 0) (= (+ |p'?3| (- p) -4) 0))))
