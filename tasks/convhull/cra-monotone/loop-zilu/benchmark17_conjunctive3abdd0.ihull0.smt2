(declare-const term_to_project_onto Real)
(declare-const k Int)
(declare-const |k'| Int)
(declare-const i Int)
(declare-const n Int)
(declare-const term_to_project_onto0 Real)
(declare-const |i'| Int)
(assert (and (<= (- i) 0) (<= (- k) 0) (= (+ k (- i)) 0) (<= (+ (- n) i 1) 0)
               (= (+ |k'| (- k) -1) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |k'| (- k)))))
