(declare-const j Int)
(declare-const cont_aux Int)
(assert (exists
         ((|cont_aux'?1| Int) (|j'?2| Int) (tr?3 Int) (tr?4 Int)
            (phi_cont_aux?5 Int) (tr?6 Int) (tr?7 Int))
         (and (<= (- cont_aux) 0) (<= (- j) 0) (<= (+ j -1) 0)
                (or (and (or (<= (+ (- tr?6) tr?7 1) 0)
                               (<= (+ tr?6 (- tr?7) 1) 0))
                           (= (+ (- phi_cont_aux?5) cont_aux) 0))
                      (and (= (+ (- tr?3) tr?4) 0)
                             (= (+ (- phi_cont_aux?5) cont_aux 1) 0)))
                (= (+ |cont_aux'?1| (- phi_cont_aux?5)) 0)
                (= (+ |j'?2| (- j) -1) 0))))
(check-sat)
