            (tr?6 Int) (tr?7 Int) (rh?8 Int) (i?9 Int) (n?10 Int))
         (and (<= (- rh?8) 0) (<= (+ (- i?9) 1) 0) (<= (+ (- n?10) 1) 0)
                (<= (+ (- n?10) i?9 1) 0)
