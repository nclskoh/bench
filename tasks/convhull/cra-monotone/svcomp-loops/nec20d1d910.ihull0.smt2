(declare-const term_to_project_onto Real)
(declare-const term_to_project_onto0 Real)
(declare-const i Int)
(declare-const |i'| Int)
(declare-const |j'| Int)
(declare-const n Int)
(declare-const j Int)
(assert (and (<= (- n) 0) (<= (- i) 0) (<= (- j) 0) (<= (+ i (- n)) 0)
               (= (+ |j'| (- j) -2) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |j'| (- j)))))
(check-sat)
