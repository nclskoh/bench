(declare-const |x'| Int)
(declare-const x Int)
(declare-const n Int)
(declare-const term_to_project_onto_integralized Int)
(assert (and (<= (- x) 0) (<= (+ (- n) 1) 0) (<= (+ (- n) x 1) 0)
               (= (+ |x'| (- x) -1) 0)
               (= (+ term_to_project_onto_integralized (- (+ |x'| (- x)))) 0)))
(check-sat)