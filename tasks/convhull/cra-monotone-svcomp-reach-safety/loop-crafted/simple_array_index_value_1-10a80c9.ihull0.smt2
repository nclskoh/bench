(declare-const term_to_project_onto Real)
(assert (exists
         ((|index'?1| Int) (tr?2 Int) (tr?3 Int) (rem?4 Int) (tr?5 Int)
            (index?6 Int))
         (and (<= (- index?6) 0) (<= (+ index?6 -999) 0)
                (or (and (is_int (/ index?6 2)) (<= (- index?6) 0) (= tr?5 0))
                      (and (or (<= index?6 0)
                                 (and (<= (- rem?4) 0)
                                        (or (<= (+ rem?4 1) 0)
                                              (<= (+ (- rem?4) 1) 0))
                                        (or (<= (+ rem?4 -1) 0)
                                              (<= (+ rem?4 1) 0))
                                        (is_int (/ (+ index?6 (- rem?4)) 2))))
                             (or (<= (+ tr?3 1) 0) (<= (+ (- tr?2) 1) 0))))
                (= (+ |index'?1| (- index?6) -1) 0)
                (= term_to_project_onto (+ |index'?1| (- index?6))))))
(check-sat)
