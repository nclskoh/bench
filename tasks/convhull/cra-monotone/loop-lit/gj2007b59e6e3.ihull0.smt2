(declare-const x Int)
(declare-const |x'| Int)
(declare-const havoc Int)
(declare-const term_to_project_onto0 Real)
(declare-const m Int)
(declare-const phi_m Int)
(declare-const |m'| Int)
(declare-const term_to_project_onto Real)
(declare-const n Int)
(assert (and (<= (- m) 0) (<= (- x) 0) (<= (+ (- n) x 1) 0)
               (or (and (= havoc 0) (= (+ (- phi_m) m) 0))
                     (and (or (<= (+ havoc 1) 0) (<= (+ (- havoc) 1) 0))
                            (= (+ (- phi_m) x) 0))) (= (+ |m'| (- phi_m)) 0)
               (= (+ |x'| (- x) -1) 0)
               (= term_to_project_onto0 (+ |x'| (- x)))
               (= term_to_project_onto (+ |m'| (- m)))))
(check-sat)
