(declare-const space Int)
(declare-const n Int)
(declare-const term_to_project_onto Real)
(declare-const left Int)
(declare-const |left'| Int)
(assert (and (<= (- left) 0) (<= (+ (- n) 1) 0) (<= (+ (- space) 1) 0)
               (<= (+ (- n) left 1) 0)
               (= (+ |left'| (* -2 space) (- left)) 0)
               (= term_to_project_onto (+ |left'| (- left)))))
(check-sat)
