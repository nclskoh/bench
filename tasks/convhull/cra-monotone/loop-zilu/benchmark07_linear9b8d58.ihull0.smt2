(declare-const |i'| Int)
(declare-const phi_k Int)
(declare-const |k'| Int)
(declare-const k Int)
(declare-const term_to_project_onto Real)
(declare-const n Int)
(declare-const i Int)
(declare-const term_to_project_onto0 Real)
(declare-const flag Int)
(assert (and (<= (- i) 0) (<= (+ (- n) 1) 0) (<= (+ (- n) i 1) 0)
               (or (and (= flag 0) (= (+ (- phi_k) k 2000) 0))
                     (and (or (<= (+ flag 1) 0) (<= (+ (- flag) 1) 0))
                            (= (+ (- phi_k) k 4000) 0)))
               (= (+ |k'| (- phi_k)) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |k'| (- k)))))
(check-sat)
