(declare-const delta_z Int)
(declare-const delta_y Int)
(declare-const delta_x Int)
(declare-const delta_c Int)
(assert (exists
         ((|c'?1| Int) (|y'?2| Int) (|x'?3| Int) (phi_z?4 Int) (phi_y?5 Int)
            (havoc?6 Int) (phi_x?7 Int) (havoc?8 Int) (|z'?9| Int) (c?10 Int)
            (d?11 Int) (y?12 Int) (x?13 Int) (z?14 Int))
         (and (<= (+ (- x?13) 1) 0) (<= (+ (- y?12) 1) 0)
                (or (and (or (<= (+ havoc?8 1) 0) (<= (+ (- havoc?8) 1) 0))
                           (= (+ (- phi_x?7) (- d?11) x?13) 0)
                           (= (+ (- phi_y?5) havoc?6) 0)
                           (= (+ (- phi_z?4) z?14 -1) 0))
                      (and (= havoc?8 0) (= (+ (- phi_x?7) x?13) 0)
                             (= (+ (- phi_y?5) (- d?11) y?12) 0)
                             (= (+ (- phi_z?4) z?14) 0)))
                (= (+ |c'?1| (- havoc?8)) 0) (= (+ |z'?9| (- phi_z?4)) 0)
                (= (+ |y'?2| (- phi_y?5)) 0) (= (+ |x'?3| (- phi_x?7)) 0)
                (= delta_x (+ |x'?3| (- x?13)))
                (= delta_y (+ |y'?2| (- y?12)))
                (= delta_z (+ |z'?9| (- z?14)))
                (= delta_c (+ |c'?1| (- c?10))))))
(check-sat)
