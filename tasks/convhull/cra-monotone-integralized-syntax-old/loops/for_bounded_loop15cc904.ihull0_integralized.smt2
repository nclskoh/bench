(declare-const x Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const i Int)
(declare-const n Int)
(declare-const havoc Int)
(declare-const |x'| Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto1_integralized Int)
(declare-const term_to_project_onto0_integralized Int)
(declare-const |y'| Int)
(declare-const y Int)
(assert (and (<= (- i) 0) (<= (+ (- n) 1) 0) (= (+ (- x) y) 0)
               (<= (+ (- n) i 1) 0) (= (+ x (- y)) 0)
               (or (<= (+ havoc 1) 0) (<= (+ (- havoc) 1) 0))
               (or (<= (+ havoc x (- y) 1) 0) (<= (+ (- havoc) (- x) y 1) 0))
               (= (+ |y'| (- havoc)) 0) (= (+ |x'| (- havoc) (- x) y) 0)
               (= (+ |i'| (- i) -1) 0)
               (= (+ term_to_project_onto1_integralized (- (+ |i'| (- i)))) 0)
               (= (+ term_to_project_onto0_integralized (- (+ |x'| (- x)))) 0)
               (= (+ term_to_project_onto_integralized (- (+ |y'| (- y)))) 0)))
(check-sat)