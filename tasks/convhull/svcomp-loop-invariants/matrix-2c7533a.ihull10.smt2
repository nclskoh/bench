(declare-const |j'| Int)
(declare-const |k'| Int)
(declare-const N_LIN Int)
(declare-const N_COL Int)
(assert (exists ((|k'?1| Int) (K?2 Int) (j?3 Int))
         (and (<= (- N_LIN) 0) (<= (- N_COL) 0) (<= (- j?3) 0)
                (<= (+ j?3 (- N_COL) 1) 0) (= (+ |k'?1| (- K?2)) 0)
                (or (and (= K?2 0) (= (- |k'?1|) 0))
                      (and (<= (+ (- K?2) 1) 0) (<= (+ (- N_LIN) 1) 0)
                             (<= (+ (- |k'?1|) 1) 0)
                             (<= (+ |k'?1| (- N_LIN)) 0))) (<= (- K?2) 0)
                (<= (- N_LIN) 0) (<= (- |k'?1|) 0)
                (<= (+ (- |k'?1|) N_LIN) 0) (= (+ |k'| (- |k'?1|)) 0)
                (= (+ |j'| (- j?3) -1) 0))))
(check-sat)
