(declare-const i Int)
(declare-const k Int)
(declare-const j Int)
(declare-const |k'| Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const n Int)
(assert (and (<= (+ (- k) 1) 0) (<= (- j) 0) (<= (+ (- i) 1) 0)
               (<= (+ (- n) 1) 0) (<= (+ (- j) k 1) 0)
               (<= (+ (* -2 n) (- i) k) 0) (= (+ |k'| (- k) -1) 0)
               (= (+ term_to_project_onto_integralized (- (+ |k'| (- k)))) 0)))
(check-sat)