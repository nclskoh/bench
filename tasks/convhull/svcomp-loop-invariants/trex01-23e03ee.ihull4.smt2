(declare-const |c'| Int)
(declare-const |y'| Int)
(declare-const d Int)
(declare-const |x'| Int)
(declare-const |z'| Int)
(assert (exists
         ((phi_z?1 Int) (phi_y?2 Int) (havoc?3 Int) (phi_x?4 Int)
            (havoc?5 Int) (y?6 Int) (x?7 Int) (z?8 Int))
         (and (<= (+ (- x?7) 1) 0) (<= (+ (- y?6) 1) 0)
                (or (and (or (<= (+ havoc?5 1) 0) (<= (+ (- havoc?5) 1) 0))
                           (= (+ (- phi_x?4) (- d) x?7) 0)
                           (= (+ (- phi_y?2) havoc?3) 0)
                           (= (+ (- phi_z?1) z?8 -1) 0))
                      (and (= havoc?5 0) (= (+ (- phi_x?4) x?7) 0)
                             (= (+ (- phi_y?2) (- d) y?6) 0)
                             (= (+ (- phi_z?1) z?8) 0)))
                (= (+ |c'| (- havoc?5)) 0) (= (+ |z'| (- phi_z?1)) 0)
                (= (+ |y'| (- phi_y?2)) 0) (= (+ |x'| (- phi_x?4)) 0))))
(check-sat)
