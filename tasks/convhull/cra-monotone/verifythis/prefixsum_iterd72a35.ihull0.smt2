(declare-const |i'| Int)
(declare-const term_to_project_onto Real)
(declare-const n Int)
(declare-const i Int)
(declare-const tr Int)
(declare-const |sum'| Int)
(declare-const tr0 Int)
(declare-const term_to_project_onto0 Real)
(declare-const sum Int)
(assert (and (<= (- i) 0) (<= (+ (- n) i 1) 0) (= (+ (- tr) sum) 0)
               (= (+ (- tr0) |sum'| (- sum)) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |sum'| (- sum)))))
(check-sat)
