(declare-const term_to_project_onto Real)
(declare-const j Int)
(declare-const |j'| Int)
(declare-const i Int)
(declare-const k Int)
(declare-const term_to_project_onto0 Real)
(declare-const |i'| Int)
(assert (and (<= (- i) 0) (<= (- j) 0) (= (+ j (- i)) 0) (<= (+ (- k) i) 0)
               (= (+ |j'| (- j) -1) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |j'| (- j)))))
(check-sat)
