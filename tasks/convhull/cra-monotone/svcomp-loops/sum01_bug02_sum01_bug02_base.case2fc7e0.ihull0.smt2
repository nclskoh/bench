(declare-const i Int)
(declare-const term_to_project_onto0 Real)
(declare-const sn Int)
(declare-const |i'| Int)
(declare-const phi_sn Int)
(declare-const term_to_project_onto Real)
(declare-const |sn'| Int)
(declare-const n Int)
(assert (and (<= (- n) 0) (<= (+ (- i) 1) 0) (<= (+ i (- n)) 0)
               (or (and (or (<= (+ i -3) 0) (<= (+ (- i) 5) 0))
                          (= (+ (- phi_sn) sn 2) 0))
                     (and (= (+ i -4) 0) (= (+ (- phi_sn) -10) 0)))
               (= (+ |sn'| (- phi_sn)) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |sn'| (- sn)))))
(check-sat)
