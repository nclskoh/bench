(assert (exists
         ((|i'?1| Int) (|rh'?2| Int) (tr?3 Int) (tr?4 Int) (phi_rh?5 Int)
            (tr?6 Int) (tr?7 Int))
         (and (<= (- rh) 0) (<= (+ (- i) 1) 0) (<= (+ (- n) 1) 0)
