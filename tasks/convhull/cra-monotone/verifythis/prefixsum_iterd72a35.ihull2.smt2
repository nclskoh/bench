(declare-const term_to_project_onto Real)
(declare-const right Int)
(declare-const space Int)
(declare-const |right'| Int)
(declare-const n Int)
(assert (and (<= (+ (- right) 1) 0) (<= (+ (- space) 1) 0)
               (<= (+ (- n) right 1) 0)
               (= (+ |right'| (* -2 space) (- right)) 0)
               (= term_to_project_onto (+ |right'| (- right)))))
(check-sat)
