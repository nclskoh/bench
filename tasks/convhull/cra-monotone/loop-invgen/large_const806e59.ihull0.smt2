(declare-const n Int)
(declare-const k Int)
(declare-const term_to_project_onto Real)
(declare-const j Int)
(declare-const |j'| Int)
(declare-const |k'| Int)
(declare-const term_to_project_onto0 Real)
(assert (and (<= (- j) 0) (<= (- k) 0) (<= (- n) 0) (<= (+ (- n) j 1) 0)
               (<= (+ (- k) 1) 0) (= (+ |j'| (- j) -1) 0)
               (= (+ |k'| (- k) 1) 0)
               (= term_to_project_onto0 (+ |k'| (- k)))
               (= term_to_project_onto (+ |j'| (- j)))))
(check-sat)
