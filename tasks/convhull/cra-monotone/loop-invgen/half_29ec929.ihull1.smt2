(declare-const i Int)
(declare-const n Int)
(declare-const |k'| Int)
(declare-const term_to_project_onto Real)
(declare-const term_to_project_onto0 Real)
(declare-const |i'| Int)
(declare-const k Int)
(assert (and (<= (- i) 0) (= (+ (* -2 k) (* 2 n) (- i)) 0)
               (<= (+ (- n) i 1) 0) (= (+ |k'| (- k) 1) 0)
               (= (+ |i'| (- i) -2) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |k'| (- k)))))
(check-sat)
