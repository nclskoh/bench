(declare-const tr0 Int)
(declare-const k Int)
(declare-const SIZE Int)
(declare-const |k'| Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const tr Int)
(assert (and (<= (+ (- k) 1) 0) (<= (- SIZE) 0) (<= (+ (- SIZE) k 1) 0)
               (<= (+ (- tr0) tr) 0) (= (+ |k'| (- k) -1) 0)
               (= (+ term_to_project_onto_integralized (- (+ |k'| (- k)))) 0)))
(check-sat)