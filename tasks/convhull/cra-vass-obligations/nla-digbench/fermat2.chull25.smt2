(declare-const |r'| Int)
(declare-const |u'| Int)
(declare-const K Int)
(declare-const |v'| Int)
(assert (exists ((u?1 Int) (v?2 Int) (r?3 Int))
         (and (= (+ -3 v?2) 0) (<= 0 (+ -1 r?3))
                (= (* -2 K) (+ (- (+ |v'| (- v?2))) (- (+ |u'| (- u?1)))))
                (<= (* -2 K) (- (+ |v'| (- v?2)))) (<= 0 (+ |v'| (- v?2)))
                (or (and (= K 0) (= r?3 |r'|) (= v?2 |v'|) (= u?1 |u'|))
                      (<= 1 K)) (<= 0 K))))
(check-sat)
