(declare-const delta_i1 Int)
(declare-const delta_i2 Int)
(assert (exists
         ((|i1'| Int) (|S1| Real) (|i2'?3| Int) (KSUM1 Int)
            (|K1| Int) (K6 Int) (|i2'?7| Int) (i2 Int) (n9 Int)
            (i1 Int))
         (and (<= (- i2) 0) (<= (- i1) 0) (< (+ (- n9) i1 2) 0)
                (<= (- K6) 0) (<= (- |K1|) 0) (= (+ |K1| (- K6)) 0)
                (<= (+ KSUM1 (- K6)) 0)
                (= (+ (- |S1|) |i2'?3| (- |K1|)) 0)
                (= (+ (- KSUM1) |K1|) 0) (= |S1| 0)
                (or (and (= K6 0) (= (- |i2'?3|) 0))
                      (and (<= (+ (- K6) 1) 0) (<= (+ (- n9) 1) 0)
                             (<= (+ |i2'?3| (- n9)) 0)
                             (<= (+ (- |i2'?3|) 1) 0)))
                (= (+ |i2'?3| (- K6)) 0)
		(<= (- K6) 0) (<= (- |i2'?3|) 0)
                (< (- n9) 0) (<= (+ (- |i2'?3|) n9) 0)
                (= (+ |i2'?7| (- |i2'?3|)) 0) (= (+ |i1'| (- i1) -1) 0)
                (= delta_i1 (+ |i1'| (- i1)))
                (= delta_i2 (+ |i2'?7| (- i2))))))
