(declare-const x Int)
(declare-const term_to_project_onto Real)
(declare-const phi_x Int)
(declare-const |y'| Int)
(declare-const term_to_project_onto0 Real)
(declare-const |x'| Int)
(declare-const y Int)
(declare-const phi_y Int)
(assert (and (<= (+ y x 2) 0)
               (or (and (<= x 0) (= (+ (- phi_x) x) 0)
                          (= (+ (- phi_y) y 1) 0))
                     (and (<= (+ (- x) 1) 0) (= (+ (- phi_x) x 1) 0)
                            (= (+ (- phi_y) y) 0))) (= (+ |y'| (- phi_y)) 0)
               (= (+ |x'| (- phi_x)) 0)
               (= term_to_project_onto0 (+ |x'| (- x)))
               (= term_to_project_onto (+ |y'| (- y)))))
(check-sat)
