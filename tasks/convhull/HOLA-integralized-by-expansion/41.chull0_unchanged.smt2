(declare-const delta_i Int)
(declare-const delta_j Int)
(assert (exists
         ((|j'?1?1| Int) (|i'?2?2| Int) (j?3?3 Int) (i?4?4 Int) (n?5?5 Int))
         (and (<= (- j?3?3) 0) (<= (- i?4?4) 0) (<= (- n?5?5) 0)
                (<= (+ (- n?5?5) i?4?4) 0)
                (= (+ |j'?1?1| (- i?4?4) (- j?3?3) -1) 0)
                (= (+ |i'?2?2| (- i?4?4) -1) 0)
                (= (+ delta_i (- (+ |i'?2?2| (- i?4?4)))) 0)
                (= (+ delta_j (- (+ |j'?1?1| (- j?3?3)))) 0))))