(declare-const sum Int)
(declare-const |sum'| Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto0 Real)
(declare-const term_to_project_onto Real)
(declare-const n Int)
(declare-const i Int)
(assert (and (<= (- i) 0) (<= (- n) 0) (<= (- sum) 0) (<= (+ (- n) i 1) 0)
               (= (+ |sum'| (- sum) (- i)) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |sum'| (- sum)))))
(check-sat)
