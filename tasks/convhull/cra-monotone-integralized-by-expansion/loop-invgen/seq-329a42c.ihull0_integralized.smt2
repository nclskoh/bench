(declare-const n1 Int)
(declare-const j1 Int)
(declare-const k Int)
(declare-const |j1'| Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const term_to_project_onto0_integralized Int)
(declare-const |k'| Int)
(declare-const n0 Int)
(assert (and (<= (- j1) 0) (<= (- k) 0) (<= (+ (- n1) (- n0) j1 1) 0)
               (<= (+ (- k) 1) 0) (= (+ |j1'| (- j1) -1) 0)
