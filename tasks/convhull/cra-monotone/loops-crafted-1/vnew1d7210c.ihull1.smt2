(declare-const n Int)
(declare-const k Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto Real)
(declare-const term_to_project_onto0 Real)
(declare-const |k'| Int)
(declare-const i Int)
(assert (and (<= (- k) 0) (<= (- i) 0) (<= (- n) 0)
               (= (+ (* 3 n) (- i) (* -3 k)) 0) (<= (+ (- n) i 1) 0)
               (= (+ |k'| (- k) 1) 0) (= (+ |i'| (- i) -3) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |k'| (- k)))))
(check-sat)
