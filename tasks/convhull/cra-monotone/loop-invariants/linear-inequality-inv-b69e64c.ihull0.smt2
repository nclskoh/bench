(declare-const i Int)
(declare-const term_to_project_onto Real)
(declare-const havoc Int)
(declare-const n Int)
(declare-const s Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto1 Real)
(declare-const term_to_project_onto0 Real)
(declare-const |v'| Int)
(declare-const |s'| Int)
(declare-const v Int)
(assert (and (<= (- i) 0) (<= (+ (- n) i 1) 0) (= (+ |i'| (- i) -1) 0)
               (= (+ (- havoc) (- s) |s'|) 0) (= (+ |v'| (- havoc)) 0)
               (= term_to_project_onto1 (+ |v'| (- v)))
               (= term_to_project_onto0 (+ |s'| (- s)))
               (= term_to_project_onto (+ |i'| (- i)))))
(check-sat)
