(declare-const tr0 Int)
(declare-const j Int)
(declare-const n Int)
(declare-const |j'| Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const tr Int)
(assert (and (<= (+ (- j) 1) 0) (<= (+ (- n) 1) 0) (<= (+ (- n) j 1) 0)
               (or (<= (+ (- tr0) tr 1) 0) (<= (+ tr0 (- tr) 1) 0))
               (= (+ |j'| (- j) -1) 0)
               (= (+ term_to_project_onto_integralized (- (+ |j'| (- j)))) 0)))
(check-sat)