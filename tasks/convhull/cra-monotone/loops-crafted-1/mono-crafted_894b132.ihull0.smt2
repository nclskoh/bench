(declare-const z Int)
(declare-const term_to_project_onto Real)
(declare-const x Int)
(declare-const y Int)
(declare-const |z'| Int)
(declare-const term_to_project_onto0 Real)
(declare-const phi_z Int)
(declare-const |x'| Int)
(assert (and (<= (+ (- y) 1) 0) (<= (- x) 0) (= (+ (- y) 10000000) 0)
               (<= (+ x (- y) 1) 0)
               (or (and (<= (+ x -4999999) 0) (= (+ (- phi_z) z) 0))
                     (and (<= (+ (- x) 5000000) 0) (= (+ (- phi_z) z -1) 0)))
               (= (+ |z'| (- phi_z)) 0) (= (+ |x'| (- x) -1) 0)
               (= term_to_project_onto0 (+ |x'| (- x)))
               (= term_to_project_onto (+ |z'| (- z)))))
(check-sat)
