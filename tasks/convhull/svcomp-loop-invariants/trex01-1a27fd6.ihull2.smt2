(declare-const delta_x Int)
(declare-const delta_z Int)
(declare-const delta_c Int)
(declare-const delta_y Int)
(assert (exists
         ((|c'?1| Int) (|z'?2| Int) (|y'?3| Int) (|x'?4| Int) (phi_z?5 Int)
            (phi_y?6 Int) (havoc?7 Int) (phi_x?8 Int) (havoc?9 Int)
            (c?10 Int) (d?11 Int) (x?12 Int) (y?13 Int) (z?14 Int))
         (and (<= (+ (- x?12) 1) 0) (<= (+ (- y?13) 1) 0)
                (or (and (or (<= (+ havoc?9 1) 0) (<= (+ (- havoc?9) 1) 0))
                           (= (+ (- phi_x?8) (- d?11) x?12) 0)
                           (= (+ (- phi_y?6) havoc?7) 0)
                           (= (+ (- phi_z?5) z?14 -1) 0))
                      (and (= havoc?9 0) (= (+ (- phi_x?8) x?12) 0)
                             (= (+ (- phi_y?6) (- d?11) y?13) 0)
                             (= (+ (- phi_z?5) z?14) 0)))
                (= (+ |c'?1| (- havoc?9)) 0) (= (+ |z'?2| (- phi_z?5)) 0)
                (= (+ |y'?3| (- phi_y?6)) 0) (= (+ |x'?4| (- phi_x?8)) 0)
                (= delta_x (+ |x'?4| (- x?12)))
                (= delta_y (+ |y'?3| (- y?13)))
                (= delta_z (+ |z'?2| (- z?14)))
                (= delta_c (+ |c'?1| (- c?10))))))
(check-sat)
