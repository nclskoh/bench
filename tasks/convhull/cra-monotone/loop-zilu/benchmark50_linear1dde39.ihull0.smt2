(declare-const ya Int)
(declare-const term_to_project_onto0 Real)
(declare-const |ya'| Int)
(declare-const |xa'| Int)
(declare-const term_to_project_onto Real)
(declare-const xa Int)
(assert (and (<= (+ (- xa) 1) 0) (= (+ (- ya) |ya'| -1) 0)
               (= (+ |xa'| (- xa) 1) 0)
               (= term_to_project_onto0 (+ |xa'| (- xa)))
               (= term_to_project_onto (+ |ya'| (- ya)))))
(check-sat)
