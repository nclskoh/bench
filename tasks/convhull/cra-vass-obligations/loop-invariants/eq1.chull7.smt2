(declare-const w Int)
(declare-const x Int)
(declare-const z Int)
(declare-const y Int)
(assert (exists ((|z'?1| Int) (|y'?2| Int) (|x'?3| Int) (|w'?4| Int))
         (or (and (= (+ |w'?4| (- |x'?3|)) (+ w (- x)))
                    (= (+ |y'?2| (- |z'?1|)) (+ y (- z)))
                    (= (+ |w'?4| (- |x'?3|)) 0) (= (+ |y'?2| (- |z'?1|)) 0)
                    (= |w'?4| (+ w 1)) (= |x'?3| (+ x 1)) (= |y'?2| y)
                    (= |z'?1| z) (= (+ |w'?4| (- |x'?3|)) 0)
                    (= (+ |y'?2| (- |z'?1|)) 0))
               (and (= (+ |w'?4| (- |x'?3|)) 0) (= (+ |y'?2| (- |z'?1|)) 0)
                      (= (+ |w'?4| (- |x'?3|)) 0) (= (+ |y'?2| (- |z'?1|)) 0)
                      (= |w'?4| w) (= |x'?3| x) (= |y'?2| (+ y -1))
                      (= |z'?1| (+ z -1))
                      (= (+ |w'?4| (- |x'?3|)) (+ w (- x)))
                      (= (+ |y'?2| (- |z'?1|)) (+ y (- z)))))))
(check-sat)
