(declare-const i Int)
(declare-const m Int)
(declare-const tr0 Int)
(declare-const |i'| Int)
(declare-const tr Int)
(declare-const term_to_project_onto Real)
(assert (and (<= (- i) 0) (<= (+ (- m) i 1) 0) (= (+ (- tr0) tr) 0)
               (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto (+ |i'| (- i)))))
(check-sat)
