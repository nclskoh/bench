(declare-const q Int)
(declare-const j Int)
(declare-const n Int)
(declare-const tr0 Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const |j'| Int)
(declare-const tr Int)
(assert (and (<= (- j) 0) (<= (+ (- n) j 1) 0)
               (or (<= (+ (- q) tr 1) 0) (<= (+ (- tr0) q 1) 0))
               (= (+ |j'| (- j) -1) 0)
               (= (+ term_to_project_onto_integralized (- (+ |j'| (- j)))) 0)))
(check-sat)