(declare-const delta_j Int)
(declare-const delta_cont_aux Int)
(assert (exists ((|cont_aux'?1| Int) (|j'?2| Int) (cont_aux?3 Int) (j?4 Int))
         (and (or (and (= |j'?2| (+ j?4 1)) (= |j'?2| 1) (= |j'?2| (+ j?4 1))
                         (= |cont_aux'?1| (+ cont_aux?3 1)) (= |j'?2| 1))
                    (and (= |j'?2| 1) (= |j'?2| 1) (= |j'?2| (+ j?4 1))
                           (= |cont_aux'?1| cont_aux?3) (= |j'?2| (+ j?4 1))))
                (= delta_j (+ |j'?2| (- j?4)))
                (= delta_cont_aux (+ |cont_aux'?1| (- cont_aux?3))))))
(check-sat)
