(declare-const delta_counter Int)
(declare-const delta_c Int)
(declare-const delta_y Int)
(declare-const delta_tmp Int)
(declare-const delta_x Int)
(assert (exists
         ((|tmp'?1| Int) (|c'?2| Int) (|y'?3| Int) (|x'?4| Int)
            (|counter'?5| Int) (c?6 Int) (x?7 Int) (y?8 Int) (tmp?9 Int)
            (counter?10 Int))
         (and (= |counter'?5| (+ counter?10 1)) (= |c'?2| (+ c?6 1))
                (= (+ |counter'?5| (- |c'?2|)) -1)
                (= (+ |c'?2| (- |tmp'?1|)) 2)
                (= delta_counter (+ |counter'?5| (- counter?10)))
                (= delta_x (+ |x'?4| (- x?7))) (= delta_y (+ |y'?3| (- y?8)))
                (= delta_c (+ |c'?2| (- c?6)))
                (= delta_tmp (+ |tmp'?1| (- tmp?9))))))
(check-sat)
