(declare-const x Int)
(declare-const phi_x Int)
(declare-const term_to_project_onto Real)
(declare-const |x'| Int)
(assert (and (<= (- x) 0) (<= (+ x -268435454) 0)
               (or (and (<= (+ (- x) 65520) 0) (= (+ (- phi_x) x 2) 0))
                     (and (<= (+ x -65519) 0) (= (+ (- phi_x) x 1) 0)))
               (= (+ |x'| (- phi_x)) 0)
               (= term_to_project_onto (+ |x'| (- x)))))
(check-sat)
