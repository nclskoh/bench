(declare-const i Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto_integralized Int)
(assert (and (<= (- i) 0) (or (<= (+ i -999999) 0) (<= (+ (- i) 1000001) 0))
               (<= (+ i -1000000) 0) (= (+ |i'| (- i) -1) 0)
               (= (+ term_to_project_onto_integralized (- (+ |i'| (- i)))) 0)))
(check-sat)