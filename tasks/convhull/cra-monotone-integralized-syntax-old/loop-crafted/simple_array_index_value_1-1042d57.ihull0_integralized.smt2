(declare-const rem Int)
(declare-const index Int)
(declare-const tr Int)
(declare-const tr1 Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const |index'| Int)
(declare-const tr0 Int)
(assert (exists
         ((standardize_int?1 Int) (standardize_int?2 Int)
            (quotient_integralized?3 Int) (quotient_integralized?4 Int))
         (and (<= (- index) 0) (<= (+ index -999) 0)
                (or (and (= standardize_int?1 quotient_integralized?3)
                           (<= (- index) 0) (= tr 0))
                      (and (or (<= index 0)
                                 (and (<= (- rem) 0)
                                        (or (<= (+ rem 1) 0)
                                              (<= (+ (- rem) 1) 0))
                                        (or (<= (+ rem -1) 0)
                                              (<= (+ rem 1) 0))
                                        (= standardize_int?2 quotient_integralized?4)))
                             (or (<= (+ tr0 1) 0) (<= (+ (- tr1) 1) 0))))
                (= (+ |index'| (- index) -1) 0)
                (= (+ term_to_project_onto_integralized
                        (- (+ |index'| (- index)))) 0)
                (= (+ index (- rem)) (* 2 quotient_integralized?4))
                (= index (* 2 quotient_integralized?3)))))