(declare-const n0 Int)
(declare-const term_to_project_onto0 Real)
(declare-const |k'| Int)
(declare-const |j1'| Int)
(declare-const term_to_project_onto Real)
(declare-const j1 Int)
(declare-const k Int)
(declare-const n1 Int)
(assert (and (<= (- j1) 0) (<= (- k) 0) (<= (+ (- n1) (- n0) j1 1) 0)
               (<= (+ (- k) 1) 0) (= (+ |j1'| (- j1) -1) 0)
               (= (+ |k'| (- k) 1) 0)
               (= term_to_project_onto0 (+ |k'| (- k)))
               (= term_to_project_onto (+ |j1'| (- j1)))))
(check-sat)
