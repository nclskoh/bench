(declare-const k Int)
(declare-const term_to_project_onto0_integralized Int)
(declare-const n Int)
(declare-const i Int)
(declare-const phi_n Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto_integralized Int)
(declare-const |n'| Int)
(declare-const rem Int)
(assert (exists
         ((standardize_int?1 Int) (standardize_int?2 Int)
            (quotient_integralized?3 Int) (quotient_integralized?4 Int))
         (and (<= (- n) 0) (<= (- i) 0) (<= (+ (* -2 k) i 1) 0)
                (or (and (or (<= i 0)
                               (and (<= (- rem) 0)
                                      (or (<= (+ rem 1) 0)
                                            (<= (+ (- rem) 1) 0))
                                      (or (<= (+ rem -1) 0) (<= (+ rem 1) 0))
                                      (= standardize_int?1 quotient_integralized?3)))
                           (= (+ (- phi_n) n) 0))
                      (and (= standardize_int?2 quotient_integralized?4)
                             (<= (- i) 0) (= (+ (- phi_n) n 1) 0)))
                (= (+ |n'| (- phi_n)) 0) (= (+ |i'| (- i) -1) 0)
                (= (+ term_to_project_onto0_integralized (- (+ |i'| (- i)))) 0)
                (= (+ term_to_project_onto_integralized (- (+ |n'| (- n)))) 0)
                (= i (* 2 quotient_integralized?4))
                (= (+ i (- rem)) (* 2 quotient_integralized?3)))))
(check-sat)