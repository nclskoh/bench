         (and (<= (+ (- N_LIN) 1) 0) (<= (+ (- N_COL) 1) 0) (<= (- j) 0)
                (= (+ N_LIN -1) 0) (= (+ N_COL -1) 0)
                (<= (+ j (- N_COL) 1) 0) (= (+ |k'?2| (- K?3)) 0)
