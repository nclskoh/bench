(declare-const term_to_project_onto Real)
(declare-const |j'| Int)
(declare-const j Int)
(declare-const phi_j Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto0 Real)
(declare-const phi_i Int)
(declare-const i Int)
(assert (and (<= (+ (- j) i 1) 0)
               (or (and (<= (+ (- j) (* 2 i)) 0) (= (+ (- phi_i) i) 0)
                          (= (+ (- phi_j) j (- i)) 0))
                     (and (<= (+ j (* -2 i) 1) 0) (= (+ (- phi_i) j (- i)) 0)
                            (= (+ (- phi_j) i) 0))) (= (+ |j'| (- phi_j)) 0)
               (= (+ |i'| (- phi_i)) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |j'| (- j)))))
(check-sat)
