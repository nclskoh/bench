(declare-const n Int)
(declare-const |y'| Int)
(declare-const |i'| Int)
(declare-const |x'| Int)
(assert (exists ((havoc?1 Int) (x?2 Int) (y?3 Int) (i?4 Int))
         (and (<= (- i?4) 0) (<= (+ (- n) 1) 0) (= (+ x?2 (- y?3)) 0)
                (<= (+ (- n) i?4 1) 0) (= (+ x?2 (- y?3)) 0)
                (or (<= (+ havoc?1 1) 0) (<= (+ (- havoc?1) 1) 0))
                (or (<= (+ havoc?1 x?2 (- y?3) 1) 0)
                      (<= (+ (- havoc?1) (- x?2) y?3 1) 0))
                (= (+ |y'| (- havoc?1)) 0)
                (= (+ |x'| (- havoc?1) (- x?2) y?3) 0)
                (= (+ |i'| (- i?4) -1) 0))))
(check-sat)
