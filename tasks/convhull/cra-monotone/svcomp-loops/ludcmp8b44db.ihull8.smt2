(declare-const i Int)
(declare-const w Real)
(declare-const |j'| Int)
(declare-const n Int)
(declare-const |w'| Real)
(declare-const tr Real)
(declare-const |w'0| Real)
(declare-const term_to_project_onto0 Real)
(declare-const |i'| Int)
(declare-const term_to_project_onto Real)
(declare-const |j'0| Int)
(declare-const j Int)
(declare-const term_to_project_onto1 Real)
(declare-const K Int)
(assert (and (<= (+ (- i) 1) 0) (<= (+ (- n) i) 0) (= (+ (- |j'0|) K) 0)
               (or (and (= K 0) (= (+ (- |w'0|) tr) 0) (= (- |j'0|) 0))
                     (and (<= (+ (- K) 1) 0) (<= (+ (- i) 1) 0)
                            (<= (+ (- |j'0|) 1) 0) (<= (+ |j'0| (- i)) 0)))
               (<= (- K) 0) (<= (- |j'0|) 0) (<= (+ (- i) 1) 0)
               (<= (+ (- |j'0|) i) 0) (= (+ |w'| (- |w'0|)) 0)
               (= (+ |j'| (- |j'0|)) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto1 (+ |i'| (- i)))
               (= term_to_project_onto0 (+ |j'| (- j)))
               (= term_to_project_onto (+ |w'| (- w)))))
(check-sat)
