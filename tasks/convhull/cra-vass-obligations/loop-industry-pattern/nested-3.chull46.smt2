(declare-const last Int)
(declare-const |st'| Int)
(declare-const |c'| Int)
(declare-const |a'| Int)
(declare-const |tmp'| Int)
(declare-const K Int)
(declare-const |b'| Int)
(assert (exists ((tmp?1 Int) (st?2 Int) (b?3 Int) (a?4 Int) (c?5 Int))
         (and (= (+ (- a?4) b?3) 0) (= (+ -1 tmp?1) 0) (= (+ -1 st?2) 0)
                (= (+ -200000 c?5) 0) (= (+ -200000 last) 0)
                (<= 0 (+ -2 a?4)) (= (* -2 K) (- (+ |a'| (- a?4))))
                (= (* -2 K) (- (+ |b'| (- b?3))))
                (or (and (= K 0) (= tmp?1 |tmp'|) (= st?2 |st'|) (= c?5 |c'|)
                           (= b?3 |b'|) (= a?4 |a'|))
                      (and (<= 1 K) (= (+ (- a?4) b?3) 0)
                             (= (+ (- |a'|) |b'|) 0) (= (+ -1 |tmp'|) 0)
                             (= (+ -1 |st'|) 0) (= (+ -200000 |c'|) 0)))
                (<= 0 K))))
(check-sat)
