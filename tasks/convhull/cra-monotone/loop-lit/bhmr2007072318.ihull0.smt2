(declare-const i Int)
(declare-const phi_b Int)
(declare-const n Int)
(declare-const a Int)
(declare-const term_to_project_onto0 Real)
(declare-const term_to_project_onto1 Real)
(declare-const |b'| Int)
(declare-const term_to_project_onto Real)
(declare-const |a'| Int)
(declare-const havoc Int)
(declare-const |i'| Int)
(declare-const phi_a Int)
(declare-const b Int)
(assert (and (<= (- b) 0) (<= (- a) 0) (<= (- i) 0) (<= (- n) 0)
               (= (+ (* 3 i) (- a) (- b)) 0) (<= (+ (- n) i 1) 0)
               (or (and (= havoc 0) (= (+ (- phi_a) a 2) 0)
                          (= (+ (- phi_b) b 1) 0))
                     (and (or (<= (+ havoc 1) 0) (<= (+ (- havoc) 1) 0))
                            (= (+ (- phi_a) a 1) 0) (= (+ (- phi_b) b 2) 0)))
               (= (+ |b'| (- phi_b)) 0) (= (+ |a'| (- phi_a)) 0)
               (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto1 (+ |i'| (- i)))
               (= term_to_project_onto0 (+ |a'| (- a)))
               (= term_to_project_onto (+ |b'| (- b)))))
(check-sat)
