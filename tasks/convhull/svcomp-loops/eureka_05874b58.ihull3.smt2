(declare-const lh Int)
(declare-const n Int)
(assert (exists
         ((|lh'?1| Int) (|rh'?2| Int) (|i'?3| Int) (K?4 Int) (|i'?5| Int)
            (|rh'?6| Int))
         (and (<= (- lh) 0) (<= (+ (- n) lh 1) 0)
                (= (+ |i'?3| (- K?4) (- lh) -1) 0)
                (or (and (= K?4 0) (= (+ (- |i'?3|) lh 1) 0)
                           (= (+ (- |rh'?2|) lh) 0))
                      (and (<= (+ (- K?4) 1) 0) (<= (- lh) 0) (<= (- lh) 0)
                             (<= (+ (- n) lh 2) 0) (<= (+ (- |i'?3|) 2) 0)
                             (<= (- |rh'?2|) 0) (<= (+ |i'?3| (- n)) 0)))
                (<= (- K?4) 0) (<= (- |rh'?2|) 0) (<= (+ (- |i'?3|) 1) 0)
                (<= (+ (- n) 1) 0) (<= (+ (- |i'?3|) n) 0)
                (= (+ |i'?5| (- |i'?3|)) 0) (= (+ |rh'?6| (- |rh'?2|)) 0)
                (= (+ |lh'?1| (- lh) -1) 0))))
(check-sat)
