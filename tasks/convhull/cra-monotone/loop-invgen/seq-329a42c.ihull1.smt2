(declare-const term_to_project_onto0 Real)
(declare-const |k'| Int)
(declare-const |i1'| Int)
(declare-const i1 Int)
(declare-const term_to_project_onto Real)
(declare-const k Int)
(declare-const n1 Int)
(assert (and (<= (- i1) 0) (<= (- k) 0) (<= (+ (- n1) i1 1) 0)
               (= (+ |i1'| (- i1) -1) 0) (= (+ |k'| (- k) -1) 0)
               (= term_to_project_onto0 (+ |k'| (- k)))
               (= term_to_project_onto (+ |i1'| (- i1)))))
(check-sat)
