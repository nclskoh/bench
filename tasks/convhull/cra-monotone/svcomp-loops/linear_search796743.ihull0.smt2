(declare-const q Int)
(declare-const n Int)
(declare-const tr0 Int)
(declare-const j Int)
(declare-const tr Int)
(declare-const |j'| Int)
(declare-const term_to_project_onto Real)
(declare-const phi_j Int)
(assert (and (<= (- j) 0) (<= (+ (- n) j 1) 0)
               (or (<= (+ (- q) tr 1) 0) (<= (+ (- tr0) q 1) 0))
               (or (and (or (<= (+ j -18) 0) (<= (+ (- j) 20) 0))
                          (= (+ (- phi_j) j 1) 0))
                     (and (= (+ j -19) 0) (= (+ (- phi_j) 4294967295) 0)))
               (= (+ |j'| (- phi_j)) 0)
               (= term_to_project_onto (+ |j'| (- j)))))
(check-sat)
