(declare-const n Int)
(declare-const j Int)
(declare-const |k'| Int)
(declare-const term_to_project_onto Real)
(declare-const term_to_project_onto0 Real)
(declare-const |j'| Int)
(declare-const k Int)
(assert (and (<= (- j) 0)
               (<= (+ 2
                        (ite (and (<= (- n) 0) (<= 0 n)) (mod n 2)
                             (+ (mod n 2) -2)) (- n) (* 2 j)) 0)
               (<= (+ (- k) 1) 0) (= (+ |j'| (- j) -1) 0)
               (= (+ |k'| (- k) 1) 0)
               (= term_to_project_onto0 (+ |k'| (- k)))
               (= term_to_project_onto (+ |j'| (- j)))))
(check-sat)
