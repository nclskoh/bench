(declare-const |res'| Int)
(declare-const |cnt'| Int)
(declare-const K Int)
(assert (exists ((res?1 Int) (cnt?2 Int))
         (and (<= 0 (+ -1 cnt?2)) (= (- K) (- (+ |res'| (- res?1))))
                (= K (- (+ |cnt'| (- cnt?2))))
                (or (and (= K 0) (= cnt?2 |cnt'|) (= res?1 |res'|)) (<= 1 K))
                (<= 0 K))))
(check-sat)
