(declare-const n0 Int)
(declare-const term_to_project_onto0 Real)
(declare-const i0 Int)
(declare-const |k'| Int)
(declare-const |i0'| Int)
(declare-const term_to_project_onto Real)
(declare-const k Int)
(assert (and (<= (- k) 0) (<= (- i0) 0) (= (+ i0 (- k)) 0)
               (<= (+ (- n0) i0 1) 0) (= (+ |k'| (- k) -1) 0)
               (= (+ |i0'| (- i0) -1) 0)
               (= term_to_project_onto0 (+ |i0'| (- i0)))
               (= term_to_project_onto (+ |k'| (- k)))))
(check-sat)
