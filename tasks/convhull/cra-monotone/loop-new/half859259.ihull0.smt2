(declare-const k Int)
(declare-const rem Int)
(declare-const n Int)
(declare-const term_to_project_onto0 Real)
(declare-const |n'| Int)
(declare-const term_to_project_onto Real)
(declare-const |i'| Int)
(declare-const phi_n Int)
(declare-const i Int)
(assert (and (<= (- n) 0) (<= (- i) 0) (<= (+ (* -2 k) i 1) 0)
               (or (and (or (<= i 0)
                              (and (<= (- rem) 0)
                                     (or (<= (+ rem 1) 0)
                                           (<= (+ (- rem) 1) 0))
                                     (or (<= (+ rem -1) 0) (<= (+ rem 1) 0))
                                     (is_int (/ (+ i (- rem)) 2))))
                          (= (+ (- phi_n) n) 0))
                     (and (is_int (/ i 2)) (<= (- i) 0)
                            (= (+ (- phi_n) n 1) 0)))
               (= (+ |n'| (- phi_n)) 0) (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto0 (+ |i'| (- i)))
               (= term_to_project_onto (+ |n'| (- n)))))
(check-sat)
