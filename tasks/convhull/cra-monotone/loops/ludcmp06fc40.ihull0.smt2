(declare-const i Int)
(declare-const w Real)
(declare-const term_to_project_onto0 Real)
(declare-const |w'| Real)
(declare-const term_to_project_onto Real)
(declare-const k Int)
(declare-const |k'| Int)
(declare-const uninterp Int)
(assert (and (<= (- k) 0) (<= (- i) 0) (<= (+ (- i) k) 0)
               (= (+ uninterp (- w) |w'|) 0) (= (+ |k'| (- k) -1) 0)
               (= term_to_project_onto0 (+ |k'| (- k)))
               (= term_to_project_onto (+ |w'| (- w)))))
(check-sat)
