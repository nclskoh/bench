(declare-const rem Int)
(declare-const term_to_project_onto1 Real)
(declare-const n Int)
(declare-const j Int)
(declare-const |l'| Int)
(declare-const term_to_project_onto Real)
(declare-const |i'| Int)
(declare-const |j'| Int)
(declare-const phi_j Int)
(declare-const term_to_project_onto0 Real)
(declare-const phi_i Int)
(declare-const i Int)
(declare-const l Int)
(assert (and (<= (- i) 0) (<= (- j) 0) (<= (- l) 0) (<= (- n) 0)
               (= (+ l (- j) (- i)) 0) (<= (+ (- n) l 1) 0)
               (or (and (or (<= l 0)
                              (and (<= (- rem) 0)
                                     (or (<= (+ rem 1) 0)
                                           (<= (+ (- rem) 1) 0))
                                     (or (<= (+ rem -1) 0) (<= (+ rem 1) 0))
                                     (is_int (/ (+ l (- rem)) 2))))
                          (= (+ (- phi_i) i) 0) (= (+ (- phi_j) j 1) 0))
                     (and (is_int (/ l 2)) (<= (- l) 0)
                            (= (+ (- phi_i) i 1) 0) (= (+ (- phi_j) j) 0)))
               (= (+ |l'| (- l) -1) 0) (= (+ |j'| (- phi_j)) 0)
               (= (+ |i'| (- phi_i)) 0)
               (= term_to_project_onto1 (+ |i'| (- i)))
               (= term_to_project_onto0 (+ |j'| (- j)))
               (= term_to_project_onto (+ |l'| (- l)))))
(check-sat)
