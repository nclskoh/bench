(declare-const i Int)
(declare-const tr Int)
(declare-const term_to_project_onto Real)
(declare-const ND Int)
(declare-const |i'| Int)
(assert (and (<= (- i) 0) (<= (+ i -102399) 0)
               (or (<= (+ (- ND) tr 1) 0) (<= (+ ND (- tr) 1) 0))
               (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto (+ |i'| (- i)))))
(check-sat)
