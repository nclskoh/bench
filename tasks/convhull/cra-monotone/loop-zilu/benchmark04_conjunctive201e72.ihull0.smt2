(declare-const j Int)
(declare-const term_to_project_onto Real)
(declare-const |j'| Int)
(declare-const term_to_project_onto0 Real)
(declare-const n Int)
(declare-const k Int)
(declare-const |k'| Int)
(assert (and (<= (+ (- n) 1) 0) (<= (- j) 0) (<= (+ j (- n) 1) 0)
               (= (+ |j'| (- j) -1) 0) (= (+ (- k) |k'| 1) 0)
               (= term_to_project_onto0 (+ |k'| (- k)))
               (= term_to_project_onto (+ |j'| (- j)))))
(check-sat)
