(declare-const term_to_project_onto Real)
(assert (exists ((|x'?1| Int) (phi_x?2 Int) (x?3 Int))
         (and (<= (- x?3) 0) (<= (+ x?3 -99999999) 0)
                (or (and (<= (+ (- x?3) 10000000) 0)
                           (= (+ (- phi_x?2) x?3 2) 0))
                      (and (<= (+ x?3 -9999999) 0)
                             (= (+ (- phi_x?2) x?3 1) 0)))
                (= (+ |x'?1| (- phi_x?2)) 0)
                (= term_to_project_onto (+ |x'?1| (- x?3))))))
(check-sat)
