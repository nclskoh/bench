(declare-const n Int)
(declare-const |y'| Int)
(declare-const y Int)
(declare-const term_to_project_onto1 Real)
(declare-const term_to_project_onto0 Real)
(declare-const havoc Int)
(declare-const |i'| Int)
(declare-const term_to_project_onto Real)
(declare-const |x'| Int)
(declare-const x Int)
(declare-const i Int)
(assert (and (<= (- i) 0) (<= (+ (- n) 1) 0) (= (+ (- x) y) 0)
               (<= (+ (- n) i 1) 0) (= (+ x (- y)) 0)
               (or (<= (+ havoc 1) 0) (<= (+ (- havoc) 1) 0))
               (or (<= (+ havoc x (- y) 1) 0) (<= (+ (- havoc) (- x) y 1) 0))
               (= (+ |y'| (- havoc)) 0) (= (+ |x'| (- havoc) (- x) y) 0)
               (= (+ |i'| (- i) -1) 0)
               (= term_to_project_onto1 (+ |i'| (- i)))
               (= term_to_project_onto0 (+ |x'| (- x)))
               (= term_to_project_onto (+ |y'| (- y)))))
(check-sat)
