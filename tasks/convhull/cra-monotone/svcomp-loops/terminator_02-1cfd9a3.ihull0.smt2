(declare-const |z'| Int)
(declare-const |x'| Int)
(declare-const z Int)
(declare-const phi_z Int)
(declare-const x Int)
(declare-const phi_x Int)
(declare-const term_to_project_onto Real)
(declare-const term_to_project_onto0 Real)
(declare-const havoc Int)
(assert (and (<= (+ x -99) 0) (<= (+ (- z) 101) 0)
               (or (and (= havoc 0) (= (+ (- phi_x) x -1) 0)
                          (= (+ (- phi_z) z -1) 0))
                     (and (or (<= (+ havoc 1) 0) (<= (+ (- havoc) 1) 0))
                            (= (+ (- phi_x) x 1) 0) (= (+ (- phi_z) z) 0)))
               (= (+ |z'| (- phi_z)) 0) (= (+ |x'| (- phi_x)) 0)
               (= term_to_project_onto0 (+ |x'| (- x)))
               (= term_to_project_onto (+ |z'| (- z)))))
(check-sat)
