(declare-const |j'| Int)
(declare-const |k'| Int)
(declare-const K Int)
(declare-const m Int)
(assert (exists ((k?1 Int) (j?2 Int))
         (and (<= 0 (+ -1 j?2)) (<= 0 (+ -1 k?1)) (<= 0 (+ (+ -1 m) (- j?2)))
                (= (- K) (- (+ |k'| (- k?1)))) (= (- K) (- (+ |j'| (- j?2))))
                (or (and (= K 0) (= j?2 |j'|) (= k?1 |k'|)) (<= 1 K))
                (<= 0 K))))
(check-sat)
